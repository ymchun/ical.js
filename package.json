{
  "name": "@ymchun/ics",
  "version": "0.0.1-alpha.1",
  "description": "A ics file parser and generator based on RFC-5545",
  "homepage": "github:ymchun/ics",
  "author": "Jean M.C.",
  "license": "MIT",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && ttsc && npm run compile-nearley-prod",
    "compile-nearley-prod": "mkdir -p ./dist/nearley/ && nearleyc ./src/nearley/grammar.ne -o ./dist/nearley/grammar.js",
    "compile-nearley": "nearleyc ./src/nearley/grammar.ne -o ./src/nearley/grammar.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src/**/*.ts",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "spell:check": "cspell '**/*.{ts}'",
    "start": "ts-node -r tsconfig-paths/register --files src/dev.ts",
    "test": "jest --no-cache --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymchun/ics.git"
  },
  "keywords": [
    "ics",
    "parser",
    "generator"
  ],
  "bugs": {
    "url": "https://github.com/ymchun/ics/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/nearley": "^2.11.1",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "coveralls": "^3.1.0",
    "cspell": "^4.1.0",
    "eslint": "^7.10.0",
    "jest": "^26.4.2",
    "snyk": "^1.404.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^3.9.7",
    "typescript-transform-paths": "^2.0.1"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.10",
    "nearley": "^2.19.7",
    "rrule": "2.6.4"
  }
}
